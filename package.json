{
  "homepage": "https://na-ovoce.cz",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsmesami/naovoce"
  },
  "dependencies": {
    "frontend-dependencies": "1.1.3"
  },
  "frontendDependencies": {
    "target": "src/naovoce/static/components",
    "packages": {
      "jquery": {
        "version": "3.3.1",
        "src": "dist/jquery.min.js"
      },
      "bootstrap": {
        "version": "3.3.7"
      },
      "leaflet": {
        "version": "1.3.3",
        "src": "{dist/leaflet.css,dist/leaflet.js}",
        "target": "src/naovoce/static/components/leaflet"
      },
      "leaflet-hash": {
        "version": "0.2.1",
        "src": "leaflet-hash.js",
        "target": "src/naovoce/static/components/leaflet"
      },
      "leaflet.markercluster": {
        "version": "1.4.1",
        "src": "{dist/MarkerCluster.css,dist/leaflet.markercluster.js}",
        "target": "src/naovoce/static/components/leaflet"
      },
      "leaflet.locatecontrol": {
        "version": "0.62.0",
        "src": "{dist/L.Control.Locate.css,dist/L.Control.Locate.min.js}",
        "target": "src/naovoce/static/components/leaflet"
      },
      "esri-leaflet": {
        "version": "2.2.2",
        "src": "dist/esri-leaflet.js",
        "target": "src/naovoce/static/components/leaflet"
      },
      "esri-leaflet-geocoder": {
        "version": "2.2.13",
        "src": "{dist/esri-leaflet-geocoder.css,dist/esri-leaflet-geocoder.js}",
        "target": "src/naovoce/static/components/leaflet"
      },
      "nanoscroller": {
        "version": "0.8.7",
        "src": "{bin/css/nanoscroller.css,bin/javascripts/jquery.nanoscroller.js}",
        "namespaced": true
      },
      "prefixfree": {
        "version": "1.0.0",
        "src": "prefixfree.min.js"
      },
      "slick-carousel": {
        "version": "1.8.1",
        "src": "slick/slick.min.js"
      },
      "mobile-detect": {
        "version": "1.4.2",
        "src": "mobile-detect.min.js"
      }
    }
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/frontend-dependencies"
  }
}
