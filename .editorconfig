root = true


[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
line_length=120

[*.py]
multi_line_output=0
lines_after_imports=2

[*.yml]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[Makefile*]
indent_style = tab
